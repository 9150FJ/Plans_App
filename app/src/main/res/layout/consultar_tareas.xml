<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginLeft="30dp"
    android:layout_marginBottom="30dp"
    android:layout_marginRight="30dp"
    android:id="@+id/cardViewGeneral"
    android:layout_height="wrap_content">

    <LinearLayout

        android:id="@+id/linearGeneral"
        android:background="@color/green"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:paddingLeft="15dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:paddingTop="15dp"
            android:textColor="@color/white"
            android:id="@+id/consultarTareasFragmentGrupo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2DAM"/>

        <TextView

            android:textColor="@color/white"
            android:layout_marginLeft="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="-"/>

        <TextView
            android:textColor="@color/white"

            android:id="@+id/consultarTareasFragmentModulo"
            android:layout_marginLeft="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PMDM"/>


    </LinearLayout>


    <LinearLayout
        android:paddingLeft="15dp"

        android:layout_marginTop="25dp"

        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/consultarTareasFragmentTitulo"
            android:textColor="@color/white"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Prueba comandos github"/>

    </LinearLayout>

    <LinearLayout
        android:paddingLeft="15dp"

        android:layout_width="match_parent"
        android:layout_marginTop="25dp"

        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/consultarTareasFragmentFechaInicio"
            android:textColor="@color/white"

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14/01/2024"
            android:paddingBottom="15dp"
            />
        <TextView
            android:textColor="@color/white"

            android:id="@+id/consultarTareasFragmentFechaFin"
            android:layout_marginLeft="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14/01/2024"/>

    </LinearLayout>



    </LinearLayout>



</androidx.cardview.widget.CardView>